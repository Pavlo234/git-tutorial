var MIN = 1;
var MAX = 5;
var numbers = jQuery('#product_qty_preorder');
numbers.on('keyup', function (e) {
var v = parseInt(numbers.val());
if (isNaN(v)) {
return numbers.val(MIN);
}
if (numbers.val() < MIN) {
numbers.val(MIN);
} else if (numbers.val() > MAX) {
numbers.val(MAX);
} else {
numbers.val(v);
}
});
</script><script>
