var seoWrapper = document.querySelector('[data-seo-wrapper]'),
readMoreBtn = seoWrapper.querySelector('[data-hide-text]'),
readMoreBtnText = readMoreBtn.innerHTML,
readMoreBtnTextHide = readMoreBtn.getAttribute('data-hide-text');
if (readMoreBtn && seoWrapper) {
readMoreBtn.addEventListener('click', () => {
if (seoWrapper.classList.contains('opened')) {
seoWrapper.classList.remove('opened');
readMoreBtn.innerHTML = readMoreBtnText;
