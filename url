jQuery(document).ready(function (){
var targetPos = false;
if (jQuery('.catalog-product-view .product-view .add-to-cart').length) {
targetPos = jQuery('.catalog-product-view .product-view .add-to-cart').offset().top;
}
if (jQuery('.catalog-product-view .product-view .oo-cart').length) {
targetPos = jQuery('.catalog-product-view .product-view .oo-cart').offset().top;
}
jQuery(window).scroll(function(){
var winScrollTop = jQuery(this).scrollTop();
if(winScrollTop > targetPos){
jQuery('#emerging-block').fadeIn();
jQuery('.fixed-phone__button').addClass('time-to-jump');
jQuery('#scroll-to-top').addClass('time-to-jump');
}
else if (winScrollTop <= targetPos){
jQuery('#emerging-block').fadeOut();
jQuery('.fixed-phone__button').removeClass('time-to-jump');
jQuery('#scroll-to-top').removeClass('time-to-jump');
}
});
});
