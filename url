//<![CDATA[
var productNotificationForm = new VarienForm('product_stock_form');
if (document.getElementById('action_url') !== null) var action_url = document.getElementById('action_url').value;
productNotificationForm.submit = function() {
if (this.validator.validate()) {
$q('#loading-image').show();
$q.ajax({
url: action_url,
type: "POST",
data: $q('form').serialize(),
success: function(data) {
$q('#loading-image').hide();
$q('.stock-form-heading').hide();
$q('.stock-form-inputs').hide();
$q('.stock-form-content').hide();
$q('.stock-form-actions').hide();
$q('#results').html(data);
jQuery('.fancybox-inner').css({
height: 'auto'
});
setTimeout(function() {
jQuery('.fancybox-wrap').css({
top: '40%'
});
}, 350);
},
error: function() {
alert('Error occured');
}
});
}
}.bind(productNotificationForm);
