function setPopupData(sender) {
var heading = jQuery(sender).attr('data-heading');
var desc = jQuery(sender).attr('data-desc');
var credit = jQuery(sender).attr('data-credit');
if (heading) {
jQuery('.popup-heading').find('.like_h2').text(heading);
}
if(desc) {
jQuery('.popup-heading-description').text(desc);
}
if(credit) {
jQuery('input#credit').val(credit);
}
}
